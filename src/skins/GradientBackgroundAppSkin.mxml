<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:mx="library://ns.adobe.com/flex/mx" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	
	<fx:Metadata>
		[HostComponent("spark.components.Application")]
	</fx:Metadata> 
	
	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
	</s:states>
	
	<!-- border -->
	<s:Rect left="10" right="10" top="10" bottom="10" radiusX="9" radiusY="9">
		<s:stroke>
			<mx:SolidColorStroke color="0xF69340" alpha=".5" weight="4"/>
		</s:stroke>
	</s:Rect>

	<s:Rect id="backgroundRect" left="1" right="1" top="1" bottom="1" radiusX="4" radiusY="4">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:entries>
					<s:GradientEntry color="0x035B7F" ratio="0" alpha="1"/>
					<s:GradientEntry color="0x008EC9" ratio="0.5" alpha="1"/>
					<s:GradientEntry color="0x002734" ratio="1" alpha="1"/>
				</s:entries>
			</s:LinearGradient>	
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" horizontalCenter="0" verticalCenter="0"  color="0xFFFFFF" fontWeight="bold"  left="0" right="0" top="0" bottom="0" />
</s:SparkSkin>